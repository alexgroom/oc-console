<% include header %>

<script type="text/javascript">

$(document).ready(function() { setInterval(getResults, 5000); });

function getResults() {
  $.ajax({
    url: "/history",
    success: function( result ) {
      $( "#latest" ).html( "<code>oc " + result[latest].command+ "<code><br/><code style='white-space:pre'>" + result[latest].stdout + "</code>" );
    }
  });
}

</script>

